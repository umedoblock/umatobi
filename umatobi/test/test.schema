[test_table]
id: integer primary key autoincrement unique not null

# None in Python
val_null:    null

# int in Python
val_integer: integer

# float in Python
val_real:    real

# depends on text_factory, str by default in Python
val_text:    text

# bytes in Python
val_blob:    blob

# time.time() in Python time format '2012-11-02T23:22:27.002'
now:    text

# float
elapsed_time: real
